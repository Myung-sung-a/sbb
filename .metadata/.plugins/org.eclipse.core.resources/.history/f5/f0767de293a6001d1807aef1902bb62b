package com.mysite.sbb;

import java.time.LocalDateTime;		// 자신의 시스템의 local의 시간설정


// persistence : JPA에서 사용된 어노테이션
import lombok.Getter;
import lombok.Setter;
//import jakarta.persistence.Entity;	// JPA에서 적용된 어노테이션

@Getter
@Setter
@Enti	// 자바 클래스를 DataBase의 테이블과 매핑된 클래스 : 테이블명 : question
public class Question {

	@id
	@GeneratedValue (strategy = GenerationType,IDENTITY)
	private Integer id;	// Primary Key, 시퀀스(1,1)
	
	private String subject;
	
	private String content;
	
	private LocalDateTime createDate;
}
